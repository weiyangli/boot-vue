{
  "name": "bvpage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.4.3",
    "loader": "^2.1.1",
    "sass": "^1.23.7",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2",
    "iview": "3.5.4",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-parsing-error": 0,
      "vue/no-use-v-if-with-v-for": 0,
      "vue/no-unused-components": 0,
      "vue/require-prop-type-constructor": 0,
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "no-multi-spaces": 0,
      "key-spacing": 0,
      "spaced-comment": 0,
      "space-before-function-paren": 0,
      "no-extend-native": 0,
      "func-names": 0,
      "no-var": 0,
      "vars-on-top": 0,
      "no-alert": 0,
      "space-infix-ops": 0,
      "no-param-reassign": 0,
      "guard-for-in": 0,
      "quote-props": 0,
      "no-console": 0,
      "no-else-return": 0,
      "comma-dangle": 0,
      "max-len": 0,
      "arrow-parens": 0,
      "arrow-body-style": 0,
      "no-unused-vars": 0,
      "no-restricted-syntax": 0,
      "no-undef": 0,
      "prefer-template": 0,
      "prefer-const": 0,
      "linebreak-style": 0,
      "yoda": 0,
      "prefer-destructuring": 0,
      "no-underscore-dangle": 0,
      "eqeqeq": 0,
      "no-plusplus": 0,
      "object-curly-newline": 0,
      "new-cap": 0,
      "prefer-arrow-callback": 0,
      "no-bitwise": 0,
      "no-unneeded-ternary": 0,
      "object-property-newline": 0,
      "no-continue": 0,
      "object-shorthand": 0,
      "no-unused-expressions": 0,
      "operator-assignment": 0,
      "no-script-url": 0,
      "function-paren-newline": 0,
      "no-lonely-if": 0,
      "operator-linebreak": 0,
      "no-return-assign": 0,
      "radix": 0,
      "no-multi-assign": 0,
      "dot-notation": 0,
      "no-shadow": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
